; Да се напише процедура (exists p? a b), която да проверява дали поне едно число в интервала отговаря на предиката.
; Да се напише процедура for-all p? a b), която да проверява дали всички числа в интервала отговарят на предиката.

(define (exists p? a b)
  (and (<= a b)
       (or (p? a) (exists p? (+ a 1) b))))

(define (for-all p? a b)
  (or (> a b)
      (and (p? a) (for-all p? (+ a 1) b))))